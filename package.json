{
  "name": "operato-windrose",
  "version": "1.1.1",
  "description": "Wind parameter visualization panel",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json",
    "e2e": "cypress install && grafana-e2e run",
    "e2e:update": "cypress install && grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Operato",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@grafana/e2e": "10.2.3",
    "@grafana/e2e-selectors": "10.2.3",
    "@grafana/eslint-config": "^6.0.1",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.104",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.30",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^8.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.4",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.70.0",
    "sass-loader": "14.0.0",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "10.2.3",
    "@grafana/runtime": "10.2.3",
    "@grafana/ui": "10.2.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "tslib": "2.5.0"
  }
}
